{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": {
        "0": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "masonry": {
            "type": "package",
            "package": {
                "name": "desandro/masonry",
                "version": "3.3.2",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/desandro/masonry/archive/v3.3.2.zip",
                    "type": "zip"
                }
            }
        },
        "imagesloaded": {
            "type": "package",
            "package": {
                "name": "desandro/imagesloaded",
                "version": "3.2.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/desandro/imagesloaded/archive/v3.2.0.zip",
                    "type": "zip"
                }
            }
        },
        "dropzone": {
            "type": "package",
            "package": {
                "name": "enyo/dropzone",
                "version": "4.2.0",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/enyo/dropzone",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        }
    },
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "desandro/imagesloaded": "3.2.0",
        "desandro/masonry": "3.3.2",
        "drupal/adminimal_admin_toolbar": "^1.5",
        "drupal/adminimal_theme": "^1.3",
        "drupal/better_exposed_filters": "^3.0@alpha",
        "drupal/bootstrap": "^3.5",
        "drupal/coffee": "^1.0@beta",
        "drupal/console": "~1.0",
        "drupal/contact_storage": "^1.0@beta",
        "drupal/content_browser": "^1.0@alpha",
        "drupal/core-composer-scaffold": "^8.9",
        "drupal/core-recommended": "^8.9",
        "drupal/dropzonejs": "^1.0@alpha",
        "drupal/ds": "^3.1",
        "drupal/entity_browser": "^1.0",
        "drupal/entity_browser_enhanced": "^1.0@beta",
        "drupal/file_browser": "^1.1",
        "drupal/focal_point": "^1.0@beta",
        "drupal/google_analytics": "^2.1",
        "drupal/honeypot": "^1.27",
        "drupal/image_widget_crop": "^2.0",
        "drupal/inline_entity_form": "^1.0@beta",
        "drupal/libraries": "3.x-dev",
        "drupal/media_entity_document": "^1.1",
        "drupal/media_entity_image": "^1.2",
        "drupal/media_entity_spotify": "^1.0@beta",
        "drupal/metatag": "^1.1",
        "drupal/module_filter": "^3.0",
        "drupal/page_manager": "^4.0@beta",
        "drupal/panels": "^4.2",
        "drupal/paragraphs": "^1.1",
        "drupal/pathauto": "^1.0",
        "drupal/recaptcha": "^2.2",
        "drupal/redirect": "^1.0@alpha",
        "drupal/search_api": "^1.3",
        "drupal/slick": "^1.0",
        "drupal/slick_media": "^1.0",
        "drupal/video_embed_field": "^1.5",
        "drupal/views_slideshow": "^4.5",
        "drush/drush": "^9.0.0",
        "enyo/dropzone": "^5.1",
        "webflo/drupal-finder": "^1.0.0"
    },
    "require-dev": {
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2",
        "drupal/core-dev": "^8.8",
        "jcalderonzumba/gastonjs": "~1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "mikey179/vfsstream": "~1.2",
        "phpunit/phpunit": "^7",
        "symfony/css-selector": "^3"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
      "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
	"patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        },
	"drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            },
            "locations": {
                "web-root": "web/"
            }
	}
    }
}
